	background-image: url(:/images/ui/bi_calling_hangup_pressed.png) center center;
}

#widgetCallBottom #pushButtonVideo {
	background-image:url(:/images/ui/bi_calling_video.png) center center;
	background-repeat:no-repeat;
	border-radius: 35px;
	padding-top:25px;
	color: #FFFFFF;
	font-size: 15px;
}
#widgetCallBottom #pushButtonVideo:hover{
	background-image:url(:/images/ui/bi_calling_video_hover.png) center center;
}
